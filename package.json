{
  "name": "array-enhancements",
  "version": "0.1.1",
  "description": "Many helpful functions for javascript Arrays",
  "main": "./index.js",
  "keywords": [
    "array",
    "arrays",
    "enhancements"
  ],
  "directories": {
    "lib": ".",
    "test": "test"
  },
  "scripts" : {
    "test" : "tap test/test.js",
    "coverage": "node test/code-coverage.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/llafuente/js-array-enhancements.git"
  },
  "author": {
    "name": "Luis Lafuente",
    "email": "llafuente@noboxout.com",
    "url": "http://www.noboxout.com"
  },
  "license": "MIT",
  "engine": {
    "node": ">=0.4"
  },
  "dependencies" : {},
  "devDependencies": {
    "tap" : "0.3.2",
    "ass": "*"
  },
  "jshintConfig": {
      "white": true,
      "curly": true,
      "eqeqeq": true,
      "indent": 4,
      "asi": false,
